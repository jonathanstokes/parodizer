{
  "name": "parodizer-server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "private": true,
  "author": "Jonathan Stokes",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '{src,test}/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "ts-node-dev --project ./tsconfig.json --respawn --transpile-only --debug ./src/server.ts",
    "start:test": "cd ../src && ts-node-dev --help",
    "build": "tsc",
    "populate": "ts-node --project ./tsconfig.json --transpile-only ../script/read-billboard.ts",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:unit": "RAPID_API_KEY=fake-rapid-api-key MUSIXMATCH_API_KEY=fake-mm-api-key TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register --exit 'src/**/*.test.ts'",
    "test:integration": "RAPID_API_KEY=fake-rapid-api-key MUSIXMATCH_API_KEY=fake-mm-api-key TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register --timeout 600000 --exit 'test/integration/**/*.integration-test.ts'",
    "precommit": "yarn run lint && yarn run test"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "billboard-top-100": "^2.5.9",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "p-queue": "^6.6.0",
    "split-string": "^6.1.0",
    "string-similarity": "^4.0.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7",
    "uuidv4": "^6.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.2",
    "@types/node": "^12.0.0",
    "@types/sinon": "^9.0.4",
    "@types/string-similarity": "^3.0.0",
    "@types/uuidv4": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2"
  }
}
